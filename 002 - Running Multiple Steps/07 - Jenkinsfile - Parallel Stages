pipeline {
	agent {
		docker {
			image 'node:latest'
		}
	}
	stages {
		stage("build") {
			agent {
				docker 'node:latest'
			}
			agent { docker 'maven:latest' }
			steps {
				parallel(
					"firstTask": {
					sh 'mvn --version'
				},
					"secondTask": {
					sh 'npm --version'
				})
			}
		}
	}
}
